@model List<User>

<h3>Testing Shop</h3>
<table>


<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Last Name</th>
      
    </tr>
  </thead>
  <tbody>
      @foreach (var item in @Model)
        {
        <tr>
          <th scope="row"></th>
          <td>@item.FirstName</td>
          <td>@item.LastName</td>
        </tr>
        }
    
    
  </tbody>
</table>