@model List<Shop>

<h3>Testing Shop</h3>
<table>


<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Item</th>
      <th scope="col">Shop Name</th>
      <th scope="col">Expiry Date</th>
      <th scope="col">Actions</th>
      
    </tr>
  </thead>
  <tbody>
      @foreach (var todo in @Model)
        {
        <tr>
          <th scope="row"></th>
          <td>@todo.Name</td>
          <td>@todo.ShopName</td>
          <td>@todo.ExpiryDate</td>
          <td>@Html.ActionLink("Remove", "Delete", "Todo", new { Name = @todo.Name})</td>
        </tr>
        }
    
    
  </tbody>
</table>